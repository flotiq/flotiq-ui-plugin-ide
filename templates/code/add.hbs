// Don't override fields other than 'title'
if (contentType?.name !== "blogposts") return;

const cacheKey = `${pluginInfo.id}-${contentType.name}-{{{suffix}}}`;
let element = getCachedElement(cacheKey)?.element;

if (!element) {
  element = document.createElement("div");
  // do your magic!
}

addElementToCache(element, cacheKey);

return element;
